{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": { "type": "string" },
    "onpremNetPrefix": { "type": "string" },
    "vnetPrefix": { "type": "string" },
    "vnetMgmtSubnetPrefix": { "type": "string" },
    "vnetNvaFeSubnetPrefix": { "type": "string" },
    "vnetNvaBeSubnetPrefix": { "type": "string" },
    "vnetWebSubnetPrefix": { "type": "string" },
    "vnetBizSubnetPrefix": { "type": "string" },
    "vnetDbSubnetPrefix": { "type": "string" }
  },
  "variables": {
    "vnet6subnetsTemplate": "https://raw.githubusercontent.com/mspnp/blueprints/master/ARMBuildingBlocks/ARMBuildingBlocks/Templates/bb-vnet-6subnets.json"
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "vnet6subnetsTemplate",
      "properties": {
        "mode": "Incremental",
        "templateLink": { "uri": "[variables('vnet6subnetsTemplate')]" },
        "parameters": {
          "baseName": { "value": "[parameters('baseName')]" },
          "onpremNetPrefix": { "value": "[parameters('onpremNetPrefix')]" },
          "vnetPrefix": { "value": "[parameters('vnetPrefix')]" },
          "vnetMgmtSubnetPrefix": { "value": "[parameters('vnetMgmtSubnetPrefix')]" },
          "vnetNvaFeSubnetPrefix": { "value": "[parameters('vnetNvaFeSubnetPrefix')]" },
          "vnetNvaBeSubnetPrefix": { "value": "[parameters('vnetNvaBeSubnetPrefix')]" },
          "vnetWebSubnetPrefix": { "value": "[parameters('vnetWebSubnetPrefix')]" },
          "vnetBizSubnetPrefix": { "value": "[parameters('vnetBizSubnetPrefix')]" },
          "vnetDbSubnetPrefix": { "value": "[parameters('vnetDbSubnetPrefix')]" }
        }
      }
    }
  ],
  "outputs": {
    "vnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.vnetId.value]"
    },

    "mgmtSubnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.mgmtSubnetId.value]"
    },
    "nvaFeSubnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.nvaFeSubnetId.value]"
    },
    "nvaBeSubnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.nvaBeSubnetId.value]"
    },
    "webSubnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.webSubnetId.value]"
    },
    "bizSubnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.bizSubnetId.value]"
    },
    "dbSubnetId": {
      "type": "string",
      "value": "[reference('vnet6subnetsTemplate').outputs.dbSubnetId.value]"
    }

  }
}

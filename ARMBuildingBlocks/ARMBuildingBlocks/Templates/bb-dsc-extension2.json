{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": {
      "type": "string" 
    },
    "typeHandlerVersion": {
      "type": "string",
      "defaultValue": "2.8"
    },
    "ModulesUrl": {
      "type": "string",
      "defaultValue": "https://raw.githubusercontent.com/mspnp/blueprints/master/ARMBuildingBlocks/ARMBuildingBlocks/Scripts/iisaspnet.ps1.zip",
    },
    "SasToken": {
      "type": "string",
      "defaultValue": ""
    },
    "ConfigurationFunction": {
      "type": "string",
      "defaultValue": "iisaspnet.ps1\\IISASPNET"
    }
  },
  "variables": {
    "location": "[resourceGroup().location]",
    "publisher": "Microsoft.Powershell",
    "type": "DSC",
    "typeHandlerVersion": "2.8",
    "ModulesUrl": "https://raw.githubusercontent.com/mspnp/blueprints/master/ARMBuildingBlocks/ARMBuildingBlocks/Scripts/iisaspnet.ps1.zip",
    "SasToken": "",
    "ConfigurationFunction": "iisaspnet.ps1\\IISASPNET"
  },
  "resources": [
    {
      "apiVersion": "2015-05-01-preview",
      "type": "Microsoft.Compute/virtualMachines/extensions",
      "name": "[parameters('vmName')]",
      "location": "[variables('location')]",
      "properties": {
        "publisher": "[variables('publisher')]",
        "type": "[variables('type')]",
        "typeHandlerVersion": "[variables('typeHandlerVersion')]",
        "settings": {
          "ModulesUrl": "[variables('ModulesUrl')]",
          "SasToken": "[variables('SasToken')]",
          "ConfigurationFunction": "[variables('ConfigurationFunction')]"
        }        
      }
    }
  ]
}

{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "virtualNetworkSettings": {
      "type": "object"
    },
    "routeTableSettings": {
      "type": "object"
    }
  },
  "variables": {
    "vnetName": "[parameters('virtualNetworkSettings').name]",
    "subnets": "[parameters('routeTableSettings').subnets]",
    "routeTableName": "[parameters('routeTableSettings').name]",
    "routeTableRoutes": "[parameters('routeTableSettings').routes]",
    "collectorTemplate": "[uri(deployment().properties.templateLink.uri, '../common/collector.json')]",
    "referenceTemplate": "[uri(deployment().properties.templateLink.uri, '../common/reference.json')]",
    "routeTablesRoutesTemplateName": "[concat(variables('routeTableName'), '-routes')]"
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "[variables('routeTablesRoutesTemplateName')]",
      "properties": {
        "mode": "Incremental",
        "templateLink": { "uri": "[variables('collectorTemplate')]" },
        "parameters": {
          "source": {
            "value": "[variables('routeTableRoutes')]"
          },
          "context": {
            "value": {
              "routeTableName": "[variables('routeTableName')]"
            }
          },
          "mapperTemplateUri": {
            "value": "[uri(deployment().properties.templateLink.uri, 'routeTable-routes.json')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Network/routeTables",
      "name": "[variables('routeTableName')]",
      "apiVersion": "2015-06-15",
      "location": "[resourceGroup().location]",
      "properties": {
        "routes": "[reference(variables('routeTablesRoutesTemplateName')).outputs.result.value]"
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "[concat(variables('subnets')[copyIndex()], '-ref')]",
      "copy": {
        "name": "subnet-reference-iterator",
        "count": "[length(variables('subnets'))]"
      },
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('referenceTemplate')]"
        },
        "parameters": {
          "resourceType": {
            "value": "Microsoft.Network/virtualNetworks/subnets"
          },
          "resourceName1": {
            "value": "[variables('vnetName')]"
          },
          "resourceName2": {
            "value": "[variables('subnets')[copyIndex()]]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "[concat(variables('routeTableName'), '-sn-0')]",
      "dependsOn": [
        "[variables('routeTableName')]"
      ],
      "properties": {
        "mode": "Incremental",
        "parameters": { },
        "template": {
          "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "parameters": { },
          "variables": { },
          "resources": [ ],
          "outputs": { }
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "[concat(variables('routeTableName'), '-sn-', copyindex(1))]",
      "dependsOn": [
        "[concat(variables('routeTableName'), '-sn-', copyindex())]"
      ],
      "copy": {
        "name": "[concat(variables('routeTableName'), '-sn-iterator')]",
        "count": "[length(variables('subnets'))]"
      },
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "parameters": {
          },
          "variables": {
          },
          "resources": [
            {
              "type": "Microsoft.Network/virtualNetworks/subnets",
              "apiVersion": "2016-03-30",
              "name": "[concat(variables('vnetName'), '/', variables('subnets')[copyIndex()])]",
              "location": "[resourceGroup().location]",
              "properties": {
                "addressPrefix": "[reference(concat(variables('subnets')[copyIndex()], '-ref')).outputs.reference.value.properties.addressPrefix]",
                "routeTable": {
                  "id": "[resourceId('Microsoft.Network/routeTables', variables('routeTableName'))]"
                }
              }
            }
          ],
          "outputs": {
          }
        },
        "parameters": {
        }
      }
    }
  ],
  "outputs": {
  }
}

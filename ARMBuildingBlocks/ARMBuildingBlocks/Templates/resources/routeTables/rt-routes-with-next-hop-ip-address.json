{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "source": {
      "type": "object"
    },
    "context": {
      "type": "object"
    },
    "state": {
      "type": "array",
      "defaultValue": [ ]
    },
    "iteration": {
      "type": "int",
      "defaultValue": 0
    }
  },
  "variables": {
    "instance": [
      {
        "name": "[parameters('source').name]",
        "properties": {
          "addressPrefix": "[parameters('source').properties.addressPrefix]",
          "nextHopType": "[parameters('source').properties.nextHopType]",
          "nextHopIpAddress": "[parameters('source').properties.nextHopIpAddress]"
        }
      }
    ]
  },
  "resources": [
  ],
  "outputs": {
    "collection": {
      "type": "array",
      "value": "[variables('instance')]"
    }
  }
}

{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "infrastructureSettings": {
      "type": "object"
    }
  },
  "variables": {
  },
    "resources": [
    ],
  "outputs": {
    "properties": {
      "type": "object",
      "value": {
        "connectionType": "Vnet2Vnet",
        "routingWeight": 10,
        "sharedKey": "[parameters('infrastructureSettings').connection.sharedKey]",
        "virtualNetworkGateway1": {
          "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', parameters('infrastructureSettings').connection.virtualNetworkGateway1.name)]"
        },
        "virtualNetworkGateway2": {
          "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', parameters('infrastructureSettings').connection.virtualNetworkGateway2.name)]"
        }
      }
    }
  }
}

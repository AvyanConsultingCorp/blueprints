{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": { "type": "string" },
    "dscTypeHandlerVersion": {
      "type": "string",
      "defaultValue": "2.19"
    }
  },
  "variables": {
    "uniqueString": "[uniqueString(resourceGroup().Id)]",
    "extensionTemplate": "https://raw.githubusercontent.com/mspnp/blueprints/master/ARMBuildingBlocks/ARMBuildingBlocks/Templates/bb-dsc-extension2.json"
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "[concat(parameters('vmName'),'-vm-iis-installation',variables('uniqueString'))]",
      "properties": {
        "mode": "Incremental",
        "templateLink": { "uri": "[variables('extensionTemplate')]" },
        "parameters": {
          "vmName": { "value": "[parameters('vmName')]" },
          "dscTypeHandlerVersion": { "value": "[parameters('dscTypeHandlerVersion')]" }
        }
      }
    }
  ],
  "outputs": {
  }
}
